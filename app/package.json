    "prettier": "^2.5.0",
    "rollup": "^2.59.0",
    "rollup-plugin-inject-process-env": "^1.3.1",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-serve": "^1.1.0",
    "rollup-plugin-svg": "^2.0.0",
    "rollup-plugin-vue": "^6.0.0",
